{
    "name": "Downlurls",
    "description": "Utility for downloading a list of urls opend in a current window",
    "version": "1.0",
    "permissions": ["tabs"],
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "download a list of urls"
    },
    "manifest_version": 2
}
